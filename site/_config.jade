mixin productContent
  .col12.infoProductos(ng-repeat="pr in productos", on-finish-render="slideUpd()", ng-class=" showThis ? 'openPrd' : 'closePrd' ")
    .container
      .col3.col-m12
        h3 {{pr.Producto}}
      .col9.col-m12
        .slider.multiple 
          div(ng-repeat="prs in pr.Productos")
            .prod
              .sintesis(ng-show="sintesisShow[prs.Archivo]")
                h4.titulo {{prs.Titulo}}
                p {{prs.Sintesis}}
                a(ng-click="mostrarSinstesis(false, prs.Archivo)").cerrar: i.fa.fa-close
                a(href="productos/archivos/{{prs.Archivo}}.pdf", target="_blank").downloadDoc Descargar
              .container(ng-click="mostrarSinstesis(true, prs.Archivo)")
                .col12
                  .categoria {{prs.Categoria}}
                .col6.col-t12.col-m12
                  h4.titulo {{prs.Titulo}}
                    span {{prs.tipo}}
                .col6.col-t12.col-m12: img(ng-src="productos/archivos/{{prs.Archivo}}.jpg").w100
